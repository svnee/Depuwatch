<table class="content">
  <tr>
    <th style="width: 200px;"><img src="/images/sessions.png" width="25" alt="logo"></th>
    <th style="width: 100px;">Vum</th>
    <th>Bis den</th>
  </tr>

<% @sessions.each do |session| %>
  <tr onmouseover="style.backgroundColor='#eee';"
onmouseout="style.backgroundColor='#fff'">
    <td><%= link_to session.title, session %></td>
    <td><%= session.start %></td>
    <td><%= session.end %></td>
  </tr>
<% end %>
</table>
